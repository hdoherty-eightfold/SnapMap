      <xs:sequence>
        <xs:element name="EF_Pay_Grade" maxOccurs="unbounded" minOccurs="0" type="DeleteType">
        </xs:element>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="PayGradeUniqueKey">
      <xs:selector xpath="EF_Pay_Grade" />
