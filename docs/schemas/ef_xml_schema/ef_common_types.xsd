<?xml version="1.1" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.1" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="EducationType">
    <xs:all>
      <xs:element name="school" type="xs:string" minOccurs="0"/>
      <xs:element name="degree" type="xs:string" minOccurs="0" />
      <xs:element name="major" type="xs:string" minOccurs="0" />
      <xs:element name="description" type="xs:string" minOccurs="0" />
      <xs:element name="location" type="xs:string" minOccurs="0" />
      <xs:element name="start_date" type="xs:date" minOccurs="0" />
      <xs:element name="end_date" type="xs:date" minOccurs="0" />
      <xs:element name="is_current" type="xs:boolean" minOccurs="0" />
      <xs:element name="verification_status" type="xs:string" minOccurs="0" />
      <xs:element name="school_external_id" type="xs:string" minOccurs="0" />
      <xs:element name="degree_external_id" type="xs:string" minOccurs="0" />
      <xs:element name="major_external_id" type="xs:string" minOccurs="0" />
      <xs:element name="is_graduated" type="xs:boolean" minOccurs="0" />
      <xs:element name="name_alias" type="xs:string" minOccurs="0" />
      <xs:element name="verification_by" type="xs:string" minOccurs="0" />
      <xs:element name="verification_ts" type="xs:dateTime" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="CertificationType">
    <xs:all>
      <xs:element name="certificate_name" type="xs:string" minOccurs="0"/>
      <xs:element name="issuing_authority" type="xs:string" minOccurs="0" />
      <xs:element name="proficiency" type="xs:string" minOccurs="0" />
      <xs:element name="license_no" type="xs:string" minOccurs="0" />
      <xs:element name="valid_from" type="xs:date" minOccurs="0" />
      <xs:element name="valid_to" type="xs:date" minOccurs="0" />
      <xs:element name="start_date" type="xs:date" minOccurs="0" />
      <xs:element name="end_date" type="xs:date" minOccurs="0" />
      <xs:element name="location_state" type="xs:string" minOccurs="0"/>
      <xs:element name="location_country" type="xs:string" minOccurs="0"/>
      <xs:element name="status" type="xs:string" minOccurs="0" />
      <xs:element name="comments" type="xs:string" minOccurs="0" />
      <xs:element name="verification_status" type="xs:string" minOccurs="0" />
      <xs:element name="verification_by" type="xs:string" minOccurs="0" />
      <xs:element name="verification_ts" type="xs:dateTime" minOccurs="0" />
      <xs:element name="certificate_external_id" type="xs:string" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="ExperienceType">
    <xs:all>
      <xs:element name="company" type="xs:string" minOccurs="0" />
      <xs:element name="title" type="xs:string" minOccurs="0" />
      <xs:element name="detailed_title" type="xs:string" minOccurs="0" />
      <xs:element name="description" type="xs:string" minOccurs="0" />
      <xs:element name="location" type="xs:string" minOccurs="0" />
      <xs:element name="is_current" type="xs:boolean" minOccurs="0" />
      <xs:element name="start_date" type="xs:date" minOccurs="0" />
      <xs:element name="end_date" type="xs:date" minOccurs="0" />
      <xs:element name="is_internal" type="xs:boolean" minOccurs="0" />
      <xs:element name="department" type="xs:string" minOccurs="0" />
      <xs:element name="industry" type="xs:string" minOccurs="0" />
      <xs:element name="experience_key" type="xs:string" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="ProjectType">
    <xs:all>
      <xs:element name="project_id" type="xs:string" minOccurs="0" />
      <xs:element name="system_id" type="xs:string" minOccurs="0" />
      <xs:element name="company" type="xs:string" minOccurs="0" />
      <xs:element name="title" type="xs:string" minOccurs="0" />
      <xs:element name="description" type="xs:string" minOccurs="0" />
      <xs:element name="start_date" type="xs:date" minOccurs="0" />
      <xs:element name="end_date" type="xs:date" minOccurs="0" />
      <xs:element name="links_list" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="link"  type="xs:string" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="contributors_list" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="contributor"  type="xs:string" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="skills_list" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="skill"  type="xs:string" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="custom_info" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="custom_field" type="CustomFieldType" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="LocationType">
      <xs:all>
        <xs:element name="address1" type="xs:string" minOccurs="0" />
        <xs:element name="address2" type="xs:string" minOccurs="0" />
        <xs:element name="city" type="xs:string" minOccurs="0" />
        <xs:element name="state" type="xs:string" minOccurs="0" />
        <xs:element name="country" type="xs:string" minOccurs="0" />
        <xs:element name="postal_code" type="xs:string" minOccurs="0" />
      </xs:all>
  </xs:complexType>

  <xs:complexType name="AttachmentType">
     <xs:all>
       <xs:element name="attachment_type" minOccurs="0" >
        <xs:simpleType>
           <xs:restriction base="xs:string"><xs:pattern value="resume|photo|others"/>
           </xs:restriction>
         </xs:simpleType>
       </xs:element>
       <xs:element name="attachment_id" type="xs:string" minOccurs="0" />
       <xs:element name="url" type="xs:string" minOccurs="0" />
       <xs:element name="content" type="xs:string" minOccurs="0" />
       <xs:element name="content_is_base64_encoded" type="xs:boolean" minOccurs="0" />
       <xs:element name="filepath" type="xs:string" minOccurs="0" />
       <xs:element name="s3_filepath" type="xs:string" minOccurs="0" />
       <xs:element name="last_modified_ts" type="xs:dateTime" minOccurs="0" />
       <xs:element name="application_metadata_list" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="application_metadata" type="AttachmentApplicationMetadataType" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
     </xs:all>
  </xs:complexType>

  <xs:complexType name="IntakeFormAttachmentType">
     <xs:all>
       <xs:element name="variable_id" type="xs:string"/>
       <xs:element name="content" type="xs:string" minOccurs="0" />
       <xs:element name="filename" type="xs:string" />
       <xs:element name="s3_file_path" type="xs:string" minOccurs="0" />
     </xs:all>
  </xs:complexType>

  <xs:complexType name="AttachmentApplicationMetadataType">
    <xs:all>
      <xs:element name="application_id" type="xs:string" minOccurs="0" />
      <xs:element name="job_id" type="xs:string" minOccurs="0" />
      <xs:element name="job_title" type="xs:string" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="NoteType">
     <xs:all>
       <xs:element name="creator" type="xs:string" minOccurs="0" />
       <xs:element name="creation_ts" type="xs:dateTime" minOccurs="0" />
       <xs:element name="subject" type="xs:string" minOccurs="0" />
       <xs:element name="body" type="xs:string" minOccurs="0" />
       <xs:element name="to" type="xs:string" minOccurs="0" />
       <xs:element name="sender" type="xs:string" minOccurs="0" />
       <xs:element name="cc" type="xs:string" minOccurs="0" />
       <xs:element name="visibility" type="xs:string" minOccurs="0" />
       <xs:element name="note_type">
         <xs:simpleType>
           <xs:restriction base="xs:string"><xs:pattern value="email|note|other"/>
           </xs:restriction>
         </xs:simpleType>
       </xs:element>
     </xs:all>
  </xs:complexType>

  <xs:simpleType name="GenderType">
     <xs:restriction base="xs:string"><xs:pattern value="male|female|n/a|opt out|decline to self identify"/>
     </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="RatingType">
     <xs:restriction base="xs:string"><xs:pattern value="yes|no|strong_yes|strong_no|mixed"/>
     </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PostedForType">
    <xs:restriction base="xs:string"><xs:pattern value="external|internal|all"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="UserType">
    <xs:all>
      <xs:element name="name" type="xs:string" minOccurs="0" />
      <xs:element name="email" type="xs:string" minOccurs="0" />
      <xs:element name="ats_roles_list" minOccurs="0">
          <xs:complexType>
              <xs:sequence>
                  <xs:element name="role"  type="xs:string" minOccurs="0" maxOccurs="unbounded" />
              </xs:sequence>
          </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>


  <xs:complexType name="AttributeType">
    <xs:all>
      <xs:element name="question" type="xs:string" />
      <xs:element name="answer" type="xs:string" />
      <xs:element name="note" type="xs:string" minOccurs="0" />
      <xs:element name="rating" type="RatingType" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="StageType">
    <xs:all>
      <!-- One of these three elements need to be present stage/workflow_stage/workflow_sub_stage.
           The validation is done later in code. -->
      <!-- "stage" field is marked for deprecation. We should use "workflow_stage" and "workflow_sub_stage" going forward.-->
      <xs:element name="stage" type="xs:string" minOccurs="0"/>
      <xs:element name="workflow_stage" type="xs:string" minOccurs="0"/>
      <xs:element name="workflow_sub_stage" type="xs:string" minOccurs="0"/>
      <xs:element name="stage_ts" type="xs:dateTime" />
      <xs:element name="reason" type="xs:string" minOccurs="0"/>
      <xs:element name="user"  type="UserType" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="JobType">
    <xs:all>
      <xs:element name="job_id" type="xs:string" />
      <xs:element name="job_name" type="xs:string" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="SourceType">
    <xs:all>
      <xs:element name="source_type" type="xs:string" minOccurs="0" />
      <xs:element name="source_name" type="xs:string" minOccurs="0" />
      <xs:element name="source" type="xs:string" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="PayType">
    <xs:all>
      <xs:element name="amount" type="xs:float" minOccurs="0" />
      <xs:element name="currency" type="xs:string" minOccurs="0" />
      <xs:element name="frequency" type="xs:string" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="ScorecardType">
    <xs:all>
      <xs:element name="interview_name"  type="xs:string" minOccurs="0" />
      <xs:element name="interviewed_at"  type="xs:dateTime" minOccurs="0" />
      <xs:element name="description"  type="xs:string" minOccurs="0" />
      <xs:element name="submitted_by"  type="UserType" minOccurs="0" />
      <xs:element name="submitted_at"  type="xs:dateTime" minOccurs="0" />
      <xs:element name="overall_rating" type="RatingType" minOccurs="0" />
      <xs:element name="attribute_list" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="attribute"  type="AttributeType" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <!-- QuestionAnswerType is used only in Candidate Object -->
  <xs:complexType name="QuestionAnswerType">
    <xs:all>
      <xs:element name="question_id" type="xs:string" minOccurs="0" />
      <xs:element name="question" type="xs:string" minOccurs="0" />
      <xs:element name="answer" type="xs:string" minOccurs="0" />
      <xs:element name="explanation" type="xs:string" minOccurs="0" />
      <xs:element name="question_type">
        <xs:simpleType>
          <xs:restriction base="xs:string"><xs:pattern value="text|choice|date|multi_value|numeric"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="choices" type="xs:string" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <!-- QuestionType is used only in Position Object -->
  <xs:complexType name="ChoiceType">
      <xs:all>
        <xs:element name="value" type="xs:string"/>
        <xs:element name="label" type="xs:string"/>
      </xs:all>
  </xs:complexType>

  <!-- QuestionType is used only in Position Object -->
  <xs:complexType name="QuestionType">
    <xs:all>
      <xs:element name="question_id" type="xs:string" minOccurs="1" />
      <xs:element name="question_text" type="xs:string" minOccurs="0" />
      <xs:element name="help" type="xs:string" minOccurs="0" />
      <xs:element name="is_required" type="xs:boolean" minOccurs="0" />
      <xs:element name="question_type">
        <xs:simpleType>
          <xs:restriction base="xs:string"><xs:pattern value="text|choice|date|multi_value|numeric"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="choice_list" minOccurs="0">
        <xs:complexType>
          <xs:choice>
            <xs:element name="choice"  type="ChoiceType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="choice_locale"  type="QuestionLabelType" minOccurs="0" maxOccurs="unbounded" />
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="question_locale" type="QuestionLabelType" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="QuestionnaireType">
    <xs:all>
      <xs:element name="questionnaire_id" type="xs:string"/>
      <xs:element name="question_dependency_list">
        <xs:complexType>
          <xs:sequence>
              <xs:element name="question_dependency"  type="QuestionDependencyType" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="QuestionDependencyType">
    <xs:all>
      <xs:element name="question_id" type="xs:string"/>
      <xs:element name="question_source" minOccurs="0" type="xs:string"/>
      <xs:element name="choice_value" type="xs:string"/>
      <xs:element name="child_question_id_list">
        <xs:complexType>
            <xs:sequence>
              <xs:element name="id" type="xs:string" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <!-- LangMappingType is used only in Position Object -->
  <xs:complexType name="LangMappingType">
      <xs:all>
        <xs:element name="locale" type="xs:string"/>
        <xs:element name="label" type="xs:string"/>
      </xs:all>
  </xs:complexType>

  <!-- QuestionLabelType is used only in Position Object -->
  <xs:complexType name="QuestionLabelType">
      <xs:all>
        <xs:element name="value" type="xs:string"/>
        <xs:element name="label" type="xs:string"/>
        <xs:element name="lang_mapping_list" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="lang_mapping"  type="LangMappingType" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:all>
  </xs:complexType>

  <xs:complexType name="CustomFieldType">
    <xs:all>
      <xs:element name="field_name" type="xs:string" />
      <xs:element name="data_type">
        <xs:simpleType>
          <xs:restriction base="xs:string"><xs:pattern value="string|boolean|date|dateTime|int|float"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="field_value" type="xs:string"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="CustomFieldNativeListType">
    <xs:all>
      <xs:element name="field_name" type="xs:string" />
      <xs:element name="data_type">
        <xs:simpleType>
          <xs:restriction base="xs:string"><xs:pattern value="string|boolean|date|dateTime|int|float"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="data_list">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="field_value" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="CustomFieldCompositeType">
    <xs:all>
      <xs:element name="object_name" type="xs:string" />
      <xs:element name="native_field_list" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="native_field" type="CustomFieldType" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
      </xs:element>
      <xs:element name="multi_value_field_list" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="field_value_list" type="CustomFieldNativeListType" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="CompositeObjectType">
    <xs:all>
      <xs:element name="native_field_list" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="native_field" type="CustomFieldType" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
      </xs:element>
      <xs:element name="multi_value_field_list" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="field_value_list" type="CustomFieldNativeListType" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="CustomCompositeListType">
    <xs:all>
      <xs:element name="object_list_name" type="xs:string" />
      <xs:element name="data_list">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="composite_object" type="CustomFieldCompositeType" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="CompositeObjectArrayType">
    <xs:all>
      <xs:element name="object_list_name" type="xs:string" />
      <xs:element name="data_list">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="composite_object" type="CompositeObjectType" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="BadgeType">
    <xs:all>
      <xs:element name="user_id" type="xs:string" />
      <xs:element name="badge_id" type="xs:string" />
      <xs:element name="title" type="xs:string" />
      <xs:element name="last_modified_ts" type="xs:dateTime" />
      <xs:element name="creator" type="xs:string" minOccurs="0"/>
      <xs:element name="comment" type="xs:string" minOccurs="0"/>
      <xs:element name="photo_url" type="xs:string" minOccurs="0"/>
      <xs:element name="photo_content" minOccurs="0">
        <xs:complexType>
          <xs:all>
            <xs:element name="content" type="xs:string" minOccurs="0" />
            <xs:element name="content_is_base64_encoded" type="xs:boolean" minOccurs="0" />
            <xs:element name="filepath" type="xs:string" minOccurs="0" />
          </xs:all>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="CourseAttendanceType">
    <xs:all>
      <xs:element name="title" type="xs:string" />
      <xs:element name="url" type="xs:string" minOccurs="0" />
      <xs:element name="type" type="xs:string" minOccurs="0" />
      <xs:element name="lms_course_id" type="xs:string" minOccurs="0" />
      <xs:element name="description" type="xs:string" minOccurs="0" />
      <xs:element name="provider" type="xs:string" minOccurs="0" />
      <xs:element name="is_internal" type="xs:boolean" minOccurs="0" />
      <xs:element name="language" type="xs:string" minOccurs="0" />
      <xs:element name="difficulty" type="xs:string" minOccurs="0" />
      <xs:element name="start_date" type="xs:date" minOccurs="0" />
      <xs:element name="completed_date" type="xs:date" minOccurs="0" />
      <xs:element name="status" type="xs:string" minOccurs="0" />
      <xs:element name="points_earned" type="xs:decimal" minOccurs="0" />
      <xs:element name="verified" type="xs:boolean" minOccurs="0" />
      <xs:element name="medium" type="xs:string" minOccurs="0" />
      <xs:element name="mandatory" type="xs:string" minOccurs="0" />
      <xs:element name="assigned_by" type="xs:string" minOccurs="0" />
      <xs:element name="percentage_completion" type="xs:float" minOccurs="0" />
      <xs:element name="duration_hours" type="xs:float" minOccurs="0" />
      <xs:element name="due_ts" type="xs:decimal" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="JobPostType">
    <xs:all>
      <xs:element name="posting_title" type="xs:string" />
      <xs:element name="posting_id" type="xs:string" minOccurs="0"/>
      <xs:element name="posting_status" type="xs:string" minOccurs="0"/>
      <xs:element name="posting_url" type="xs:string" minOccurs="0"/>
      <xs:element name="posted_for" default="all">
          <xs:simpleType>
              <xs:restriction base="xs:string"><xs:pattern value="external|internal|all"/>
              </xs:restriction>
          </xs:simpleType>
      </xs:element>
      <xs:element name="job_description" type="xs:string" minOccurs="0"/>
      <xs:element name="locale" type="xs:string" minOccurs="0"/>
      <xs:element name="is_primary_post" type="xs:boolean" minOccurs="0"/>
      <xs:element name="published_ts" type="xs:dateTime" />
      <xs:element name="expiration_ts" type="xs:dateTime" minOccurs="0"/>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="ApplicationType">
    <xs:all>
      <xs:element name="candidate_id"  type="xs:string" />
