      <xs:sequence>
        <xs:element name="EF_Holiday" type="DeleteType" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="HolidayUniqueKey">
      <xs:selector xpath="EF_Holiday" />
      <xs:field xpath="holiday_id" />
    </xs:unique>
  </xs:element>
</xs:schema>
