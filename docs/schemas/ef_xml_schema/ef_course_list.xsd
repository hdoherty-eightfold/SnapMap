<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="ef_course_common.xsd" />

  <xs:element name="EF_Course_List">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="EF_Course" minOccurs="0" maxOccurs="unbounded" type="IngestType">
        </xs:element>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="CourseUniqueKey">
      <xs:selector xpath="EF_Course" />
      <xs:field xpath="lms_course_id" />
    </xs:unique>
  </xs:element>
</xs:schema>
