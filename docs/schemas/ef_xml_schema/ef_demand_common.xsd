<?xml version="1.1" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.1" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="ef_common_types.xsd" />

    <xs:simpleType name="DemandPublishMode">
        <xs:restriction base="xs:string">
            <xs:enumeration value="local" />
            <xs:enumeration value="custom" />
            <xs:enumeration value="local_and_custom" />
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="IngestType">
        <xs:all>
            <xs:element name="job_id" type="NoSpaceString" />
            <xs:element name="group_id" type="NoSpaceString" minOccurs="0"/>
            <xs:element name="title" type="xs:string" />
            <xs:element name="is_open" type="xs:boolean" />
            <xs:element name="demand_description" type="xs:string" />
            <xs:element name="demand_notes" type="xs:string" minOccurs="0"/>
            <xs:element name="openings" type="OpeningsType" />
            <xs:element name="location_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="location" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="location_city" type="xs:string" minOccurs="0" />
            <xs:element name="location_state" type="xs:string" minOccurs="0" />
            <xs:element name="location_country" type="xs:string" minOccurs="0" />
            <xs:element name="demand_manager" type="UserType" minOccurs="0" />
            <xs:element name="resource_manager" type="UserType" minOccurs="0" />
            <xs:element name="follower_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="follower" type="UserType" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="demand_code" type="xs:string" minOccurs="0" />
            <xs:element name="demand_band" type="xs:string" minOccurs="0" />
            <xs:element name="skills_list" type="SkillListType" minOccurs="0" />
            <xs:element name="required_skills_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="skill" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="creation_ts" type="xs:dateTime" />
            <xs:element name="last_modified_ts" type="xs:dateTime" />
            <xs:element name="custom_info" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                    <xs:element name="custom_field" type="CustomFieldType" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="custom_multi_value_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                    <xs:element name="custom_mv_field" type="CustomFieldNativeListType" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="custom_objects_list" minOccurs="0">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="object_list" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="object" type="CustomFieldCompositeType" minOccurs="0" maxOccurs="unbounded" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="list_of_objects" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="object_list" type="CustomCompositeListType" minOccurs="0" maxOccurs="unbounded" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:all>
                </xs:complexType>
            </xs:element>
            <xs:element name="entity_tags_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="tag" type="EntityTagType" minOccurs="0" maxOccurs="20" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="demand_org_units" minOccurs="0">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="program_id" type="NoSpaceString" minOccurs="0" />
                        <xs:element name="project_id" type="NoSpaceString" minOccurs="1" />
                    </xs:all>
                </xs:complexType>
            </xs:element>
            <xs:element name="rm_user_group_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="user_group"  type="RmUserGroupType" minOccurs="1" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="demand_publish_settings" minOccurs="0">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="publish_mode" type="DemandPublishMode" minOccurs="1" />
                        <xs:element name="publish_custom_user_group_list" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="user_group"  type="RmUserGroupType" minOccurs="1" maxOccurs="unbounded" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:all>
                </xs:complexType>
            </xs:element>
            <xs:element name="role_association_settings" type="RoleAssociationSettingsForPositionType" minOccurs="0"/>
        </xs:all>
        <xs:attribute name="operation" fixed="ingest"/>
    </xs:complexType>

    <xs:complexType name="UpdateType">
        <xs:all>
            <xs:element name="job_id" type="NoSpaceString" />
            <xs:element name="group_id" type="NoSpaceString" minOccurs="0"/>
            <xs:element name="title" type="xs:string" minOccurs="0" />
            <xs:element name="is_open" type="xs:boolean" minOccurs="0" />
            <xs:element name="demand_description" type="xs:string" minOccurs="0" />
            <xs:element name="demand_notes" type="xs:string" minOccurs="0" />
            <xs:element name="openings" type="OpeningsType" minOccurs="0" />
            <xs:element name="location_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="location" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="location_city" type="xs:string" minOccurs="0" />
            <xs:element name="location_state" type="xs:string" minOccurs="0" />
            <xs:element name="location_country" type="xs:string" minOccurs="0" />
            <xs:element name="demand_manager" type="UserType" minOccurs="0" />
            <xs:element name="resource_manager" type="UserType" minOccurs="0" />
            <xs:element name="follower_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="follower" type="UserType" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="demand_code" type="xs:string" minOccurs="0" />
            <xs:element name="demand_band" type="xs:string" minOccurs="0" />
            <xs:element name="skills_list" type="SkillListType" minOccurs="0" />
            <xs:element name="required_skills_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="skill" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="creation_ts" type="xs:dateTime" minOccurs="0" />
            <xs:element name="last_modified_ts" type="xs:dateTime" minOccurs="0" />
            <xs:element name="custom_info" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                    <xs:element name="custom_field" type="CustomFieldType" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="custom_multi_value_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                    <xs:element name="custom_mv_field" type="CustomFieldNativeListType" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="custom_objects_list" minOccurs="0">
                <xs:complexType>
                    <xs:all>
                    <xs:element name="object_list" minOccurs="0">
                        <xs:complexType>
                        <xs:sequence>
                            <xs:element name="object" type="CustomFieldCompositeType" minOccurs="0" maxOccurs="unbounded" />
                        </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="list_of_objects" minOccurs="0">
                        <xs:complexType>
                        <xs:sequence>
                            <xs:element name="object_list" type="CustomCompositeListType" minOccurs="0" maxOccurs="unbounded" />
                        </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    </xs:all>
                </xs:complexType>
            </xs:element>
            <xs:element name="entity_tags_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="tag" type="EntityTagType" minOccurs="0" maxOccurs="20" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="demand_org_units" minOccurs="0">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="program_id" type="NoSpaceString" minOccurs="0" />
                        <xs:element name="project_id" type="NoSpaceString" minOccurs="1" />
                    </xs:all>
                </xs:complexType>
            </xs:element>
            <xs:element name="rm_user_group_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="user_group"  type="RmUserGroupType" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="demand_publish_settings" minOccurs="0">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="publish_mode" type="DemandPublishMode" minOccurs="0" />
                        <xs:element name="publish_custom_user_group_list" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="user_group"  type="RmUserGroupType" minOccurs="0" maxOccurs="unbounded" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:all>
                </xs:complexType>
            </xs:element>
            <xs:element name="role_association_settings" type="RoleAssociationSettingsForPositionType" minOccurs="0"/>
        </xs:all>
        <xs:attribute name="operation">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="update"/>
                    <xs:enumeration value="upsert"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="DeleteType">
        <xs:all>
            <xs:element name="job_id" type="NoSpaceString" />
        </xs:all>
        <xs:attribute name="operation" fixed="delete"/>
    </xs:complexType>

</xs:schema>
